{
  "name": "wael-moh/myfatoorah-laravel",
    "description": "Myfathoorah REST API package, Original from vmdevelopment",
    "license": "MIT",
    "authors": [
        {
            "name": "Wael Moh",
            "email": "w.m4ilb0x@gmail.com",
            "homepage": "http://www.wael-moh.com/"
        }
    ],
    "minimum-stability": "dev",

    "require": {
        "php": "^8.0|^7.1|^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": ">=6.0"
    },

    "autoload": {
        "psr-4": {
            "VMdevelopment\\MyFatoorah\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "VMdevelopment\\MyFatoorah\\Tests\\": "tests/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "VMdevelopment\\MyFatoorah\\MyFatoorahServiceProfider"
            ]
        }
    }
}
